{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: cl.exe build active file",
      "command": "cl.exe",
      "args": [
        "/Zi",
        "/EHsc",
        "/nologo",
        "/I${workspaceFolder}\\source\\private",
        "/I${workspaceFolder}\\source\\public",
        "/Fo${workspaceFolder}\\bin\\intermediates\\${fileBasenameNoExtension}.obj",
        "/Fe${workspaceFolder}\\bin\\${fileBasenameNoExtension}.exe",
        "${file}"
      ],
      "options": {
        "cwd": "${workspaceFolder}\\bin\\intermediates"
      },
      "problemMatcher": [
        "$msCompile"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated, Custom .json "
    }
  ],
  "version": "2.0.0"
}