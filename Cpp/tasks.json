{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: cl.exe build active file",
      "command": "cl.exe",
      "args": [
        "/Zi",
        "/EHsc",
        "/nologo",
        "/I${workspaceFolder}\\source\\private\\",
        "/I${workspaceFolder}\\source\\public\\",
        "/Fe${workspaceFolder}\\bin\\${fileBasenameNoExtension}.exe",
        "/permissive",
        "/Wall",
        "/WX",
        "/std:c++latest",
        "${fileDirname}/**.cpp"
      ],
      "options": {
        "cwd": "${workspaceFolder}\\bin\\intermediates"
      },
      "problemMatcher": [
        "$msCompile"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}
